a = input("Список 1: ").split()
b = input("Список 2: ").split()
a = list(int(x) for x in a if x.isdigit())
b = list(int(x) for x in b if x.isdigit())
combined = list(a + b)
combined_tuple = tuple(combined)
combined_set = set(combined)
print("Об'єднано:", combined)
print("Об'єднано:", combined_tuple)
print("Унікальні:", combined_set)
print("Відсортовані унікальні:", sorted(combined_set))
